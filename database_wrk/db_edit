package database_wrk

import (
	"fmt"
	Db "github.com/Juandiaz5673/go-scraper-test-v2/database_wrk"
)

func Db_edit() {
	fmt.Println("It worked! ")

	y := "INSERT INTO newkingjamesversion (ID, BookNumber, BookName, ChapterName, ChapterNumber, VerseNumber, VerseText, Testament, BookPreference) VALUES('1', '1', 'Genesis', 'The History of Creation', '1', '1', 'In the beginning God created the heavens and the earth.', 'old', 'New King James Version')"
	insert, err := Db.Query(y)

	if err != nil {
		panic(err.Error())
	}
	defer insert.Close()
}